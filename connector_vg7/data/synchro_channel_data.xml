<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">

    <record forcecreate="1" id="res_partner" model="synchro.channel.model">
        <field name="name">res.partner</field>
        <field name="field_uname">name</field>
        <field name="counterpart_name">customers</field>
        <field name="synchro_channel_id" ref="channel_vg7"/>
        <field name="search_keys">(['vat', 'fiscalcode', 'is_company', 'type'],
               ['vat', 'fiscalcode', 'is_company'],
               ['rea_code'],
               ['vat', 'name', 'is_company', 'type'],
               ['fiscalcode', 'type'],
               ['vat', 'is_company'],
               ['name', 'is_company'],
               ['vat'],
               ['name'],
               ['dim_name'])</field>
        <field name="field_2complete">True</field>
        <field name="sequence">10</field>
    </record>

    <record forcecreate="1" id="product_product" model="synchro.channel.model">
        <field name="name">product.product</field>
        <field name="field_uname">name</field>
        <field name="counterpart_name">products</field>
        <field name="search_keys">(['name', 'default_code'],
               ['name', 'barcode'],
               ['name'],
               ['default_code'],
               ['barcode'],
               ['dim_name'],)</field>
        <field name="synchro_channel_id" ref="channel_vg7"/>
        <field name="field_2complete">True</field>
        <field name="sequence">10</field>
    </record>

    <record forcecreate="1" id="product_template" model="synchro.channel.model">
