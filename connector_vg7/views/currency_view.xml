<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_currency_synchro_form" model="ir.ui.view">
        <field name="name">view.currency.synchro.form</field>
        <field name="model">res.currency</field>
        <field name="inherit_id" ref="base.view_currency_rate_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form/*[last()]" position="after">
                <page name="synchro" string="Synchronization">
                    <group>
                        <field name="vg7_id"/>
                        <field name="oe7_id"/>
                        <field name="oe8_id"/>
                        <field name="oe10_id"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
