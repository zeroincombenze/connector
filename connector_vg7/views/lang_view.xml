<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_lang_synchro_form" model="ir.ui.view">
        <field name="name">view.lang.synchro.form</field>
        <field name="model">res.lang</field>
        <field name="inherit_id" ref="base.res_lang_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group" position="after">
                <group string="Synchronization">
                    <field name="vg7_id"/>
                    <field name="oe7_id"/>
                    <field name="oe8_id"/>
                    <field name="oe10_id"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>
